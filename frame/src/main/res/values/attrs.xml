<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RatingBarView">
        <attr name="starImageSize" format="dimension" />
        <attr name="starCount" format="integer" />
        <attr name="starEmpty" format="reference" />
        <attr name="starFill" format="reference" />
    </declare-styleable>

    <declare-styleable name="StateLayout">
        <attr name="loadingView" format="reference" />
        <attr name="errorView" format="reference" />
        <attr name="emptyView" format="reference" />
        <attr name="noNetworkView" format="reference" />
    </declare-styleable>

    <declare-styleable name="CommonLayoutView">
        <attr name="leftText" format="string" />
        <attr name="centerText" format="string" />
        <attr name="rightText" format="string" />
        <attr name="leftTopText" format="string" />
        <attr name="centerTopText" format="string" />
        <attr name="rightTopText" format="string" />
        <attr name="leftBottomText" format="string" />
        <attr name="centerBottomText" format="string" />
        <attr name="rightBottomText" format="string" />

        <attr name="leftTextSize" format="dimension" />
        <attr name="centerTextSize" format="dimension" />
        <attr name="rightTextSize" format="dimension" />
        <attr name="leftTopTextSize" format="dimension" />
        <attr name="centerTopTextSize" format="dimension" />
        <attr name="rightTopTextSize" format="dimension" />
        <attr name="leftBottomTextSize" format="dimension" />
        <attr name="centerBottomTextSize" format="dimension" />
        <attr name="rightBottomTextSize" format="dimension" />

        <attr name="leftTextColor" format="reference|color" />
        <attr name="centerTextColor" format="reference|color" />
        <attr name="rightTextColor" format="reference|color" />
        <attr name="leftTopTextColor" format="reference|color" />
        <attr name="centerTopTextColor" format="reference|color" />
        <attr name="rightTopTextColor" format="reference|color" />
        <attr name="leftBottomTextColor" format="reference|color" />
        <attr name="centerBottomTextColor" format="reference|color" />
        <attr name="rightBottomTextColor" format="reference|color" />

        <attr name="leftImgRes" format="reference" />
        <attr name="rightImgRes" format="reference" />
        <attr name="centerTopImgRes" format="reference" />
        <attr name="centerBottomImgRes" format="reference" />

        <attr name="leftImgWidth" format="dimension" />
        <attr name="rightImgWidth" format="dimension" />
        <attr name="centerTopImgWidth" format="dimension" />
        <attr name="centerBottomImgWidth" format="dimension" />

        <attr name="leftImgHeight" format="dimension" />
        <attr name="rightImgHeight" format="dimension" />
        <attr name="centerTopImgHeight" format="dimension" />
        <attr name="centerBottomImgHeight" format="dimension" />

        <attr name="leftImgTint" format="reference" />
        <attr name="rightImgTint" format="reference" />
        <attr name="centerTopImgTint" format="reference" />
        <attr name="centerBottomImgTint" format="reference" />

        <attr name="isShowCheckBox" format="boolean" />
        <attr name="checkBoxRes" format="reference" />
        <attr name="isBoxCheck" format="boolean" />

        <attr name="isShowSwitch" format="boolean" />
        <attr name="thumbRes" format="reference" />
        <attr name="trackRes" format="reference" />
        <attr name="isSwitchCheck" format="boolean" />

        <attr name="isShowRadio" format="boolean" />
        <attr name="radioButtonRes" format="reference" />
        <attr name="isRadioCheck" format="boolean" />

        <attr name="topLineHeight" format="dimension" />
        <attr name="bottomLineHeight" format="dimension" />
        <attr name="topLineMarginLeft" format="dimension" />
        <attr name="topLineMarginRight" format="dimension" />
        <attr name="bottomLineMarginLeft" format="dimension" />
        <attr name="bottomLineMarginRight" format="dimension" />
        <attr name="topLineColor" format="color" />
        <attr name="bottomLineColor" format="color" />

        <attr name="bottomMarginTop" format="dimension" />
        <attr name="topMarginBottom" format="dimension" />
        <attr name="marginLeft" format="dimension" />
        <attr name="marginRight" format="dimension" />
        <attr name="marginTop" format="dimension" />
        <attr name="marginBottom" format="dimension" />
    </declare-styleable>

    <declare-styleable name="InputView">
        <attr name="textColor" format="reference|color" />
        <attr name="textBackground" format="reference" />
        <attr name="textBackgroundSelected" format="reference" />
        <attr name="maxLength" format="integer" />
        <attr name="boxMargin" format="dimension" />
        <attr name="isConnect" format="boolean" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="lineColor" format="reference|color" />
        <attr name="radius" format="dimension" />
        <attr name="inputType">
            <flag name="none" value="0x00000000" />
            <flag name="text" value="0x00000001" />
            <flag name="textCapCharacters" value="0x00001001" />
            <flag name="textCapWords" value="0x00002001" />
            <flag name="textCapSentences" value="0x00004001" />
            <flag name="textAutoCorrect" value="0x00008001" />
            <flag name="textAutoComplete" value="0x00010001" />
            <flag name="textMultiLine" value="0x00020001" />
            <flag name="textImeMultiLine" value="0x00040001" />
            <flag name="textNoSuggestions" value="0x00080001" />
            <flag name="textUri" value="0x00000011" />
            <flag name="textEmailAddress" value="0x00000021" />
            <flag name="textEmailSubject" value="0x00000031" />
            <flag name="textShortMessage" value="0x00000041" />
            <flag name="textLongMessage" value="0x00000051" />
            <flag name="textPersonName" value="0x00000061" />
            <flag name="textPostalAddress" value="0x00000071" />
            <flag name="textPassword" value="0x00000081" />
            <flag name="textVisiblePassword" value="0x00000091" />
            <flag name="textWebEditText" value="0x000000a1" />
            <flag name="textFilter" value="0x000000b1" />
            <flag name="textPhonetic" value="0x000000c1" />
            <flag name="textWebEmailAddress" value="0x000000d1" />
            <flag name="textWebPassword" value="0x000000e1" />
            <flag name="number" value="0x00000002" />
            <flag name="numberSigned" value="0x00001002" />
            <flag name="numberDecimal" value="0x00002002" />
            <flag name="numberPassword" value="0x00000012" />
            <flag name="phone" value="0x00000003" />
            <flag name="datetime" value="0x00000004" />
            <flag name="date" value="0x00000014" />
            <flag name="time" value="0x00000024" />
        </attr>
    </declare-styleable>
</resources>